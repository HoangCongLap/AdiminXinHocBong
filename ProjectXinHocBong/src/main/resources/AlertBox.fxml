<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.text.TextFlow?>

<AnchorPane prefHeight="278.0" prefWidth="398.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="xinhocbong.alertbox.StudentInformationAlert">
   <children>
      <ImageView fitHeight="70.0" fitWidth="117.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@img/IG1.png" />
         </image></ImageView>
      <TextFlow fx:id="text" layoutX="49.0" layoutY="82.0" prefHeight="155.0" prefWidth="282.0" style="-fx-background-color: #CFCFCF;">
         <children>
            <Text fx:id="textData" strokeType="OUTSIDE" strokeWidth="0.0" text="setData" wrappingWidth="1.0">
               <font>
                  <Font size="15.0" />
               </font>
            </Text>
         </children>
      </TextFlow>
      <Button layoutX="310.0" layoutY="238.0" mnemonicParsing="false" onAction="#Ok" prefHeight="26.0" prefWidth="81.0" style="-fx-background-color: #5585be;" text="Ok">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Button>
      <Label alignment="CENTER" layoutX="100.0" prefHeight="70.0" prefWidth="298.0" style="-fx-background-color: #00FFFF;" text="STUDENT INFORMATION">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
